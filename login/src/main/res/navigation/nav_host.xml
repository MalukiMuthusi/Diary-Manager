<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_host"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/welcomeFragment"
        android:name="codes.malukimuthusi.login.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_welcome2Fragment"
            app:destination="@id/welcome2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/welcome2Fragment"
        android:name="codes.malukimuthusi.login.Welcome2Fragment"
        android:label="fragment_welcome2"
        tools:layout="@layout/fragment_welcome2" >
        <action
            android:id="@+id/action_welcome2Fragment_to_welcome3Fragment"
            app:destination="@id/welcome3Fragment" />
    </fragment>
    <fragment
        android:id="@+id/welcome3Fragment"
        android:name="codes.malukimuthusi.login.Welcome3Fragment"
        android:label="fragment_welcome3"
        tools:layout="@layout/fragment_welcome3" >
        <action
            android:id="@+id/action_welcome3Fragment_to_welcomeFragment4"
            app:destination="@id/welcomeFragment4" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment4"
        android:name="codes.malukimuthusi.login.WelcomeFragment4"
        android:label="fragment_welcome4"
        tools:layout="@layout/fragment_welcome4" >
        <action
            android:id="@+id/action_welcomeFragment4_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="codes.malukimuthusi.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" />
</navigation>